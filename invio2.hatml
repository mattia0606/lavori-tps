<!DOCTYPE html>
<html>
<head>
    <title>Pagina di invio 2</title>
</head>
<body>
    <script>
        // Oggetto JSON predefinito
        const persone = [
            { nome: "Mario", telefono: "1234567890" },
            { nome: "Luca", telefono: "9876543210" },
            { nome: "Anna", telefono: "5555555555" }
        ];

        // Conversione in JSON
        const personeJSON = JSON.stringify(persone);

        // Redirezione alla pagina ricevente
        window.location.href = "receiver2.html?data=" + personeJSON;
    </script>
</body>
</html>
